package brushup;

import java.util.Arrays;
import java.util.HashSet;
import java.util.List;
import java.util.Set;


public class RdfromArray {

	public static void main(String[] args) {
		Integer[] numberArr =  { 10, 30, 30, 10, 15, 13, 34, 15 };

		//List<Integer> uniqueList = Arrays.asList(numberArr).stream().distinct().collect(Collectors.toList());
		//for (Integer singleElement : uniqueList) {
		//	System.out.println(singleElement);
		
		List<Integer> unique = Arrays.asList(numberArr);
		
		Set<Integer> ele = new HashSet<>(unique);
		for(Integer ele1:ele) {
			System.out.println(ele1);
		}
		
		}

	}




