package brushup;

public class ComparisonSyntax {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s1 ="hello is my name";
		String s2 ="hello";
		String s3="meklo";
		String s4="hemlo";
		System.out.println(s1.compareTo(s2));
		System.out.println(s1.compareTo(s3));
		System.out.println(s1.compareTo(s4));
		int a = s1.indexOf("name");
	int a1 = s1.indexOf("m");
	System.out.println(a1+"jhjhjhjhjhjhjh");
		System.out.println(a+"jjkj");
		System.out.println(s1.endsWith("name"));

	}

}
